pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

local t
local speed
local plane = {}
local lives
local particles

function add_particle( particle )
	add(particles, particle)
end

function del_particle( particle )
	del(particles, particle)
end

function update_particles( )
	for particle in all(particles) do
		particle:update()
		if particle.ttl and particle.ttl < 0 then
			del_particle(particle)
		end
		if particle.x < -8 or particle.x >= 136
			or particle.y < -8 or particle.y >= 136 then
			del_particle(particle)
		end
	end
end

function draw_particles( )
	for particle in all(particles) do
		particle:draw()
	end
end

function make_smoke( x, y)
	local res = { x = x, y = y, r = 2 }
	function res:update( )
		self.x -= speed
		self.y -= 1/2
		self.r += 1/4
		self.x += rnd(2) - 1
		self.y += rnd(2) - 1
	end
	function res:draw( )
		circfill(self.x, self.y, self.r, 0)
	end
	add_particle(res)
end

function plane:draw( )
	sprite_num = 0
	if self.vy > 1 then sprite_num = 2 end
	if self.vy < -1 then sprite_num = 4 end
	spr(sprite_num, self.x, self.y - 4, 2, 2)
end

function plane:update( )
	
	-- controls
	if btn(0) then self.vx -= 1 end
	if btn(1) then self.vx += 1 end
	if btn(2) then self.vy -= 1 end
	if btn(3) then self.vy += 1 end

	-- make the plane oscillate a bit
	self.vy += sin(t / 30) / 10

	-- dampening
	self.vx *= 0.8
	self.vy *= 0.8

	-- basic physics and bound checking
	local x = self.x + self.vx
	local y = self.y + self.vy
	x_max = 128 - self.width
	y_max = 128 - self.height
	if x < 0 or x >= x_max then
		x = flr(self.x / x_max + 0.5) * x_max
		self.vx = 0
	end
	if y < 0 or y >= y_max then
		y = flr(self.y / y_max + 0.5) * y_max
		self.vy = 0
	end
	self.x = x
	self.y = y

	if btnp(4) then lives = (lives - 1) % 4 end

	-- particles
	if lives <= 1 then
		for i=0,speed-1 do
			make_smoke(self.x + 8 - i * (self.vx + speed) / speed, self.y + 4 - i * self.vy / speed)
		end
	end


end

function print_bordered( string, x, y, fc, bc )
	for i=-1,1 do
		for j=-1,1 do
			print(string, x + i, y + j, bc)
		end
	end
	print(string, x, y, fc)
end

function display_hud( )
	print_bordered(t, 2, 2, 7, 4)
	print_bordered(speed, 2, 121, 7, 4)
	local str = "" .. #particles
	local len = #str
	print_bordered(str, 127 - 4 * len, 121, 7, 4)
	for i=1,3 do
		if lives >= i then
			spr(14, 96 + i * 8, 1)
		else
			spr(15, 96 + i * 8, 1)
		end
	end
end


local clouds_bg = {}
local clouds_fg = {}
function init_clouds( )
	for i=0,17 do
		add(clouds_bg, {x = i * 8, y = rnd(8), r = rnd(8) + 6})
		add(clouds_bg, {x = i * 8, y = 128 - rnd(8), r = rnd(8) + 6})
		add(clouds_fg, {x = i * 8, y = rnd(8) - 4, r = rnd(8) + 4})
		add(clouds_fg, {x = i * 8, y = 132 - rnd(8), r = rnd(8) + 4})
	end
end

function update_clouds( )
	for cloud in all(clouds_bg) do
		cloud.x -= speed / 2
		if cloud.x <= -8 then
			del(clouds_bg, cloud)
			if cloud.y < 64 then
				add(clouds_bg, {x = 17 * 8, y = rnd(8), r = rnd(8) + 6})
			else
				add(clouds_bg, {x = 17 * 8, y = 128 - rnd(8), r = rnd(8) + 6})
			end
		end
	end
	for cloud in all(clouds_fg) do
		cloud.x -= speed
		if cloud.x <= -8 then
			del(clouds_fg, cloud)
			if cloud.y < 64 then
				add(clouds_fg, {x = 17 * 8, y = rnd(8) - 4, r = rnd(8) + 4})
			else
				add(clouds_fg, {x = 17 * 8, y = 132 - rnd(8), r = rnd(8) + 4})
			end
		end
	end
end

function draw_sky( )
	rectfill(0, 0, 127, 127, 12)
	for cloud in all(clouds_bg) do
		circfill(cloud.x, cloud.y, cloud.r, 15)
	end
	for cloud in all(clouds_fg) do
		circfill(cloud.x, cloud.y, cloud.r, 7)
	end
end

function _init( )
	plane.x = 64 - 8
	plane.y = 64 - 4
	plane.vx = 0
	plane.vy = 0
	plane.width = 16
	plane.height = 8
	t = 0
	speed = 6
	lives = 2
	particles = {}
	init_clouds()
end

function _update( )
	speed = (speed + 0.001)
	update_clouds()
	update_particles()
	plane:update()
	t += 1
end

function _draw( )
	cls()
	draw_sky()
	draw_particles()
	plane:draw()
	display_hud()
end

